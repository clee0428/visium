# robust saver: always works (PNG + PDF)
save_plot <- function(p, file_base, w=7, h=5, dpi=300) {
  png(file_base %+% ".png", width = w*dpi, height = h*dpi, res = dpi)
  print(p)
  dev.off()

  pdf(file_base %+% ".pdf", width = w, height = h, useDingbats = FALSE)
  print(p)
  dev.off()
}
`%+%` <- function(a,b) paste0(a,b)


p16_umap_clusters <- DimPlot(
  seu_16um,
  reduction = "umap",
  group.by = "seurat_clusters",
  label = TRUE,
  repel = TRUE
) + theme_classic() + ggtitle("PB245 16¬µm ‚Äì UMAP (Seurat clusters)")

save_plot(p16_umap_clusters, file.path(fig_dir_16, "PB245_16um_UMAP_seurat_clusters"), w=7, h=5)

p16_spatial_clusters <- SpatialDimPlot(
