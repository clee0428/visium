3C) Bar plot composition (counts + %)
df_comp <- as.data.frame(table(seu_16um$compartment))
colnames(df_comp) <- c("compartment","n_bins")
df_comp$percent <- 100 * df_comp$n_bins / sum(df_comp$n_bins)

p_bar <- ggplot(df_comp, aes(x=reorder(compartment, -percent), y=percent)) +
  geom_col() +
  geom_text(aes(label=sprintf("%.1f%%", percent)), vjust=-0.2, size=4) +
  theme_classic() +
  labs(x=NULL, y="Percent of 16¬µm bins", title="PB245 16¬µm ‚Äì Compartment composition") +
  coord_cartesian(ylim=c(0, max(df_comp$percent)*1.15))

ggsave(file.path(fig_dir_16, "PB245_16um_Composition_barplot.png"),
       p_bar, width=7, height=5, dpi=300)

write.csv(df_comp,
          file.path(tab_dir_16, "PB245_16um_compartment_composition.csv"),
          row.names = FALSE)

